<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup Condition="'$(SolutionDir)' == ''">
		<SolutionDir>..\$(ProjectDir)</SolutionDir>
	</PropertyGroup>

	<PropertyGroup>
		<Configurations>1.3;1.3_DEBUG;1.4;1.4_DEBUG</Configurations>
		<ModSharedProps>$(SolutionDir)Shared.mod.props.csproj</ModSharedProps>
		<MorzivaSharedProps>$(SolutionDir)Morziva.mod.props.csproj</MorzivaSharedProps>
		<AssemblyName>Morziva.FactionSupplement</AssemblyName>
		<RootNamespace>Morziva</RootNamespace>
	</PropertyGroup>

	<Import Project="$(ModSharedProps)" />
	<Import Project="$(MorzivaSharedProps)" />

</Project>